<div class="main-column">

<:
	die "Falta la página del manual" unless -f "$(TORSVNHEAD)/doc/tor.1.in";
	my $man = `man2html -M '' $(TORSVNHEAD)/doc/tor.1.in`;
	die "No hay página de manual" unless $man;

	$man =~ s,.*<body>,,is;
	$man =~ s,</body>.*,,is;

	print $man;
:>

